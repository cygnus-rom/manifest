        <project path="packages/providers/TelephonyProviders" name="packages_providers_TelephonyProviders" remote="cygnus"/>
        <project path="packages/services/Telephony" name="packages_services_Telephony" remote="cygnus"/>
     <!-- Device -->
        <project path="device/qcom/common" name="device_qcom_common" remote="cygnus-old" />
        <project path="device/qcom/sepolicy" name="device_qcom_sepolicy" remote="cygnus-old"/>
        <project path="device/cygnus/sepolicy" name="device_cygnus_sepolicy" remote="cygnus-old"/>
        <project path="device/cygnus/common" name="device_cygnus_common" remote="cygnus"/>
     
     <!-- System -->
        <project path="system/core" name="system_core" remote="cygnus"/>
        <project path="system/vold" name="system_vold" remote="cygnus"/>
        <project path="system/sepolicy" name="system_sepolicy" remote="cygnus"/>
     
     <!-- Manifest -->
   	<project path="manifest" name="manifest" remote="cygnus"/>

     <!-- Hardware -->
